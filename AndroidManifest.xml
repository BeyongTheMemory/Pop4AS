<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pop"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />

    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <!-- SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>


    <uses-feature android:name="android.hardware.camera" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false"></uses-feature>
    、
    <uses-feature
        android:name="android.hardware.camera.flash"
        android:required="false"></uses-feature>
    <uses-feature
        android:name="android.hardware.camera.front"
        android:required="false"></uses-feature>


    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true"></supports-screens>

    <application
        android:name="com.pop.activity.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/login_pop"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Light">

        <activity
            android:name="com.pop.activity.ScheduActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.pop.activity.LoginActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.RegistActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.MainActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.PersonActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.FilterPopActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.MessageActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name=".activity.NewPopPhotoActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.NewPop_yuyin_Activity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.NewPop_words_Activity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.OptionActivity"
            android:label="@string/app_name"></activity>


        <activity
            android:name="com.pop.activity.NewPopActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.AccoutActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.AboutActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.MessageNoticeActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.TopPopActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.MyCollectActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.MyPhotoActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.pop.activity.PersonnalActivity"
            android:label="@string/app_name"></activity>


        <activity
            android:name="com.pop.activity.ModifyPwdActivity"
            android:label="@string/app_name"></activity>




        <activity
            android:name=".activity.PopInfoActivity"
            android:label="@string/app_name"
            android:theme="@style/MyDialogTopRight"></activity>

        <activity
            android:name="com.pop.activity.PopChoseActivity"
            android:label="@string/app_name"
            android:theme="@style/MyDialogTopRight"></activity>


    </application>


</manifest>
